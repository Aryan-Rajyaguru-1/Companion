{
  "version": 2,
  "builds": [
    {
      "src": "companion_baas/api/api_server.py",
      "use": "@vercel/python",
      "config": {
        "requirements": "requirements-vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/companion_baas/api/api_server.py"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))$",
      "dest": "/website/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/website/frontend/index.html"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "LOG_LEVEL": "INFO",
    "DEBUG": "false"
  }
}