# Environment Variables Template
# Copy this to .env and fill in your actual API keys
# NEVER commit .env to git!

# OpenRouter API Keys
OPENROUTER_TONGYI_KEY=your-openrouter-tongyi-key-here
OPENROUTER_DEEPSEEK_KEY=your-openrouter-deepseek-key-here
OPENROUTER_GPT_OSS_KEY=your-openrouter-gpt-oss-key-here

# Hugging Face API Key
HUGGINGFACE_API_KEY=your-huggingface-token-here

# Groq API Key
GROQ_API_KEY=your-groq-api-key-here

# Cloud Deployment API Key (for api_server.py)
API_KEY=generate-a-strong-random-key-here

# Environment
ENVIRONMENT=production
LOG_LEVEL=INFO
PORT=8000
